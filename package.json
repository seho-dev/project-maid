{
  "name": "project-maid",
  "displayName": "Project Maid",
  "publisher": "akirarika",
  "description": "ðŸš€ Power for your project!",
  "repository": "https://github.com/akirarika/project-maid",
  "version": "0.0.2",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:project-maid.helloWorld",
    "onCommand:project-maid.createTemplate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "project-maid.createTemplate",
        "title": "Create from Template.."
      },
      {
        "command": "project-maid.helloWorld",
        "title": "H"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "project-maid.createTemplate",
          "group": "1_modification",
          "when": "explorerResourceIsFolder"
        }
      ],
      "editor/title": [
        {
          "command": "project-maid.helloWorld",
          "group": "navigation"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@poech/camel-hump-under": "^1.1.0",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7",
    "js-yaml": "^4.1.0",
    "read-dir-deep": "^7.0.1"
  }
}
